<% @recipeList.each do |recipe| %>

	<h2><%= recipe["title"] %></h2>
	<strong>Ingredients:</strong>
	<ul>
	  <% recipe["ingredients"].each do |ingredient| %>
	    <li><%= ingredient %></li>
	  <% end %>
	</ul>
	<strong>Directions:</strong>
	<ol>
	  <% recipe["directions"].each do |direction| %>
	    <li><%= direction %></li>
	  <% end %>
	</ol>
<% end %>
